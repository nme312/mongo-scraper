<div class="row">
    <div class="col-md-12">
        <div class="articles">
            {{#each article}}
            <div class="jumbotron col-md-12">
                <div class="article-details col-md-6">
                    <h5>{{this.title}} </h5>
                    <h5>{{this.byLine}} </h5>
                    <h5 class="mb-3">{{this.summary}} </h5>
                </div>
                <div class="col-md-4 article-buttons">
                    <button class="btn btn-primary save-button">Save</button>
                    <button class="btn btn-primary note-button">Add Note</button>
                </div>
            </div>
            <hr> {{/each}}
        </div>
    </div>
</div>